<div class="page-header"><h1>Upload Picture</h1></div>

<%= form_for @picture do |f| %>
  <%= render 'layouts/errors', object: @picture %>

  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control'%>
  </div>

  <%= f.submit 'Upload', class: 'btn btn-primary' %>
<% end %>
